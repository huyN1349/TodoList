<%- include("header") -%>


  <div class="box" id="heading">
    <h1><%= listTitleEJS %></h1>
  </div>

  <div class="box">
    <% for (let i = 0; i < itemsEJS.length; i++) { %>
    <div class="item">
      <input type="checkbox">
      <p><%= itemsEJS[i] %></p>
    </div>

    <% } %>
    <form action="/" method="post" class="item">
      <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
      <button type="submit" name="list" value=<%= listTitleEJS %>>+</button>
    </form>
  </div>

<%- include("footer") -%>
